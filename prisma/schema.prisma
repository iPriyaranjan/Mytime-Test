datasource db {
  provider = "postgresql"   // or mysql | sqlite | sqlserver | cockroachdb
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model TestUser {
  id       Int     @id @default(autoincrement())
  userName     String
  email    String  @unique
  password String
  age Int?
  createdAt DateTime @default(now())
}
